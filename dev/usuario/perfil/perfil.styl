.Header
	width: 100%;
	height: 5em;
	background: white;
	margin-bottom: 3em;
	h2
		margin:0;
.Contenedor
		margin:0 auto;
		width:100%;
		//background:red;
	.Info
		width: 46.25em;
		background: red;
		margin: 0 auto;
		.About
			background:#404040;
			height:15em;
			width: 100%;
			display: inline-block;
			color:white;
			padding:0;
			.Iconos
				//background:red;
				margin:1em 0;
				p
					margin:0;
					text-align:right;
					i
						text-align:right;
						padding:0 0.5em 0 0.5em;
						font-size:1.3em;
			figure
				background-image:url(../img/usuario.jpg);
				background-size:cover;
				height:6em;
				width:6em;
				border-radius:100%;
				margin:1em auto;
				top:2em;
			h4
				margin:0;
				text-align:center;
		.Navegacion
			background:white;
			border-bottom:2px solid white;
			&-Contenedor
				button
					width:8.5em;
					height:2.3em;
					background:white;
					padding:0.82em 0 0.62em 0;
					border:0;
					//transition:all .3s ease-out;
				button:hover
					border-bottom:2px solid #427fed;
					font-weight:bold;
