div.Popup2(popup-close)
	section.Contenedor-Popup-Clientes
		figure.Img-Cliente
		form.Form-Cliente
			h2 Modificar Cliente
			div.Form-Cliente-Contenedor
				label Nombre:
				input(type="text" ng-model="clienteUpdate.nombre")
			div.Form-Cliente-Contenedor
				label Paterno:
				input(type="text" ng-model="clienteUpdate.nom_paterno")
			div.Form-Cliente-Contenedor
				label Materno:
				input(type="text" ng-model="clienteUpdate.nom_materno")
			div.Form-Cliente-Contenedor
				label Usuario:
				input(type="text" ng-model="clienteUpdate.usuario")
			div.Form-Cliente-Contenedor
				label Password:
				input(type="password" ng-model="clienteUpdate.password")
			div.Form-Cliente-Contenedor
				label Repetir password:
				input(type="password")
			div.Form-Cliente-Contenedor
				button Agregar