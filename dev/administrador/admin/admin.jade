section.Contenedor-Administrador
	h2 Administrador
	div.Iconos-Administrador
		form(action="http://localhost/estructura-angular/backend/pdf/administradores.php" method="POST").Form-Administrador
			i.icon-search
			input(type="text")
			button(type="submit").icon-file-pdf
			i(popup-add="<popup-agregar-administrador/>").icon-plus
	table(fixed-header).Tabla-Administrador
		thead
			tr
				th
					p Id
				th
					p Nombre
				th
					p Correo
				th
					p Usuario
		tbody
			tr(ng-repeat="admin in administradores")
				td 
					p {{admin.id_administrador}}
				td 
					p {{admin.nombre}}
				td 
					p {{admin.correo}}
				td.Td
					div
						p {{admin.usuario}}
						i(popup-add="<popup-confirmacion-delete-admin/>").icon-bin
						//i(popup-add="<popup-update-administrador administrador='admin'/>").icon-pencil